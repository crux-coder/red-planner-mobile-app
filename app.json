{
	"expo": {
		"name": "RedPlanner",
		"slug": "RedPlanner",
		"scheme": "red-planner",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "automatic",
		"assetBundlePatterns": ["**/*"],
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"config": {
				"usesNonExemptEncryption": false
			},
			"splash": {
				"image": "./assets/splash.png",
				"resizeMode": "cover",
				"backgroundColor": "#ffffff",
				"dark": {
					"backgroundColor": "#000000",
					"resizeMode": "cover",
					"image": "./assets/splash-dark.png"
				}
			},
			"bundleIdentifier": "uk.co.redlaser"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png"
			},
			"splash": {
				"image": "./assets/splash.png",
				"resizeMode": "cover",
				"backgroundColor": "#ffffff",
				"dark": {
					"backgroundColor": "#000000",
					"resizeMode": "cover",
					"image": "./assets/splash-dark.png"
				}
			},
			"package": "uk.co.redlaser"
		},
		"experiments": {
			"typedRoutes": true
		},
		"plugins": ["expo-router", "expo-secure-store"],
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "c24570eb-25fa-46a6-8a32-4a699fe86a36"
			}
		}
	}
}
